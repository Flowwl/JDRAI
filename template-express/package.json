{
  "name": "template-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=2048' ts-node src/server.ts",
    "clean": "rm -rf node_modules",
    "clean-install": "yarn clean && yarn install",
    "dev": "yarn up && ts-node-dev src/server.ts",
    "test": "NODE_ENV=test CI_MODE=activated nyc mocha --exit",
    "up": "migrate --compiler=\"ts:./ts_node_compiler.js\" up",
    "down": "migrate --compiler=\"ts:./ts_node_compiler.js\" down",
    "runDb": "docker-compose up -d --remove-orphans",
    "detect-circular": "npx madge --ts-config ./tsconfig.json --extensions js,ts --circular . "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flowwl/studyo-express.git"
  },
